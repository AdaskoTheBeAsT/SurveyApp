{
  "name": "client-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build --extra-webpack-config webpack.extra.js",
    "create-icon-sprite": "svg2sprite ./src/assets/svg ./src/assets/sprites/sprites.svg --stripAttrs id --stripTags title",
    "docs": "hads ./docs -o",
    "e2e": "testcafe chrome e2e/**/*.e2e-spec.ts --app \"yarn start\" --app-init-delay 40000 --proxy-bypass http://localhost:4200/",
    "e2e:remote": "cross-env BASE_URL=%E2E_URL% USERNAME=%USERNAME% PASSWORD=%PASSWORD% testcafe chrome e2e/**/*.e2e-spec.ts --hostname localhost --proxy-bypass %E2E_URL% --reporter spec,xunit:../../../Cake/.artifacts/frontende2etest/frontend-e2e.xunit.xml,nunit:../../../Cake/.artifacts/frontende2etest/frontend-e2e.nunit.xml,html:../../../Cake/.artifacts/frontende2etest/frontend-e2e.html",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate",
    "postinstall": "yarn run prettier",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "lint2": "ng lint ClientApp --format codeFrame --fix && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "ng": "ng",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,tsx,json,md,scss,css,less}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,tsx,json,md,scss,css,less}\"",
    "prod": "ng build --prod --aot --extra-webpack-config webpack.extra.js",
    "start": "ng serve",
    "stats": "ng build --stats-json --extra-webpack-config webpack.extra.js",
    "stats-prod": "ng build --prod --stats-json --extra-webpack-config webpack.extra.js",
    "stats-report": "webpack-bundle-analyzer ../wwwroot/stats.json",
    "style": "yarn prettier && yarn lint",
    "test": "jest --watch",
    "test:ci": "yarn run lint && jest --runInBand --coverage",
    "test:ci2": "jest --coverage",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "ts-lint": "tslint --project tsconfig.json --config tslint.json  -o ../../../Cake/.artifacts/tslint-result.json -t json --force 'src/**/*.ts'",
    "ts-lint-console": "tslint --project tsconfig.json --config tslint.json --force src/**/*.ts",
    "verify": "yarn prettier -l && yarn lint && jest --bail"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md},!src/app/api/**": [
      "prettier --write",
      "git add"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "yarn lint",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "dependencies": {
    "@angular/animations": "~10.1.4",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/forms": "~10.1.4",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/router": "~10.1.4",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.31",
    "@fortawesome/free-solid-svg-icons": "^5.15.0",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngxs/store": "^3.7.0",
    "bootstrap": "^4.5.2",
    "bootswatch": "^4.5.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.6.5",
    "lodash": "^4.17.20",
    "moment": "^2.29.0",
    "moment-timezone": "^0.5.31",
    "ng-svg-icon-sprite": "^1.9.1",
    "ngx-moment": "^5.0.0",
    "primeicons": "^4.0.0",
    "primeng": "^10.0.2",
    "rxjs": "~6.6.3",
    "svg4everybody": "^2.1.9",
    "tslib": "^2.0.1",
    "windows-iana": "^4.2.1",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular-eslint/builder": "0.5.0-beta.2",
    "@angular-eslint/eslint-plugin": "0.5.0-beta.2",
    "@angular-eslint/eslint-plugin-template": "0.5.0-beta.2",
    "@angular-eslint/schematics": "^0.5.0-beta.2",
    "@angular-eslint/template-parser": "0.5.0-beta.2",
    "@angular/cdk": "^10.2.3",
    "@angular/cli": "~10.1.4",
    "@angular/compiler-cli": "~10.1.4",
    "@angular/language-service": "~10.1.4",
    "@ngxs/devtools-plugin": "^3.7.0",
    "@ngxs/logger-plugin": "^3.7.0",
    "@types/jasmine": "^3.5.14",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.3.0",
    "codelyzer": "^6.0.1",
    "eslint": "^7.10.0",
    "hads": "^2.1.0",
    "htmlhint": "^0.14.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-html-reporter": "^3.2.0",
    "jest-multi-test-result-processor": "^1.0.0",
    "jest-nunit-reporter": "^1.3.1",
    "jest-preset-angular": "^8.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^10.4.0",
    "ng-mocks": "^10.2.0",
    "ngx-build-plus": "^10.1.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "puppeteer": "^5.3.1",
    "stylelint": "~15.10.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "^3.18.0",
    "svg2sprite-cli": "^2.0.1",
    "testcafe": "^1.9.4",
    "testcafe-angular-selectors": "^0.4.1",
    "testcafe-reporter-html": "^1.4.6",
    "testcafe-reporter-nunit": "^0.1.2",
    "testcafe-reporter-xunit": "^2.1.0",
    "ts-jest": "^26.4.1",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "4.0.3",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "jestNunitReporter": {
    "outputPath": "../../../Cake/.artifacts/frontendunittest/",
    "outputFilename": "frontend-test.nunit.xml"
  },
  "jestSonar": {
    "reportPath": "../../../Cake/.artifacts/frontendunittest/",
    "reportFile": "frontend-test.sonar.xml",
    "indent": 4
  }
}
