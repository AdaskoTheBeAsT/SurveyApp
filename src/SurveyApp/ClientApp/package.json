{
  "name": "client-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "yarn run env && ng build --prod",
    "start": "yarn run env && ng serve --aot --proxy-config proxy.conf.js",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "yarn run env && ng test",
    "test:ci": "yarn run env && yarn run lint && ng test --code-coverage --watch=false",
    "e2e": "yarn run env && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "yarn run prettier",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^7.2.3",
    "@angular/common": "^7.2.3",
    "@angular/compiler": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/forms": "^7.2.3",
    "@angular/http": "^7.2.3",
    "@angular/platform-browser": "^7.2.3",
    "@angular/platform-browser-dynamic": "^7.2.3",
    "@angular/router": "^7.2.3",
    "@ngx-translate/core": "^11.0.1",
    "@ng-bootstrap/ng-bootstrap": "^4.0.2",
    "bootstrap": "^4.2.1",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "core-js": "^2.6.3",
    "lodash": "^4.17.11",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "^7.2.3",
    "@angular/language-service": "^7.2.3",
    "@angular-devkit/build-angular": "^0.13.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^3.0.1",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "^2.0.6",
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.20",
    "codelyzer": "^4.5.0",
    "hads": "^1.7.1",
    "htmlhint": "^0.10.1",
    "https-proxy-agent": "^2.2.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "prettier": "^1.16.3",
    "tslint-config-prettier": "^1.17.0",
    "stylelint-config-prettier": "^4.0.0",
    "pretty-quick": "^1.10.0",
    "husky": "^1.3.1",
    "protractor": "~5.4.2",
    "puppeteer": "^1.11.0",
    "stylelint": "~9.10.1",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.5.1",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "typescript": "~3.2.4"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  }
}
